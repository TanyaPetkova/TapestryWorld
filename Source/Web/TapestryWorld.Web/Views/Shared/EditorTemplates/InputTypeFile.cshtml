@model HttpPostedFileBase 
<div class="row">
    <div class="col-md-3">
        @Html.LabelFor(m => m)
    </div>
    <div class="col-md-5">
        @Html.TextBoxFor(m => m, new { @class = "form-control", type = "file"})
    </div>
    <div>
        @Html.ValidationMessageFor(m => m)
    </div>
</div>
<br />
